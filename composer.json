{
    "name": "bigtechnologie/laravel-entity-tools-v2",
    "description": "Package Laravel pour générer automatiquement des entités et des CRUD via Artisan.",
    "keywords": [
        "laravel",
        "artisan",
        "entity",
        "crud",
        "generator",
        "codegen"
    ],
    "homepage": "https://github.com/BigTechnologie/laravel-entity-tools",
    "license": "MIT",
    "authors": [
        {
            "name": "Kandia Diallo",
            "email": "dev.technologie2018@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Kandia\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kandia\\EntityToolsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
